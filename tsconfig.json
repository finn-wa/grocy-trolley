{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./out-ts",
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "baseUrl": "./src",
    "paths": { "@grocy-trolley/*": ["*"] }
  }
}
